@import '../../styl/component-include.styl'

.modal-backdrop
  background-color #333 !important
  
.modal-dialog
  top 50%
  margin-top -100px
  
.modal-content 
  border none !important
  
.modal-title
  color #666 !important
  font-size 15.75pt !important
  font-weight normal !important
  text-align center !important
  
.modal .close
  background url("data:image/png;base64,") no-repeat bottom left !important
  width 33px !important
  height 33px !important
  top 30px !important
  right 30px !important
  
.popup-window-buttons .bx_bt_button
  font-weight bold !important
  padding 15px 0 !important
  text-transform uppercase !important
  background #f2f2f2 !important
  width 240px !important
  font-size 14px !important

.popup-window-buttons .bx_item_detail:last-child .bx_bt_button
  color #fff !important
  background-color #9a639a !important
  border-color #9a639a !important
    
.popup-window-buttons .bx_bt_button:hover,
.popup-window-buttons .bx_bt_button:focus,
.popup-window-buttons .bx_bt_button:active
  background #006eb9 !important
  border-color #006eb9 !important
  
.popup-window-buttons .bx_item_detail:last-child .bx_bt_button:hover
  text-decoration none !important
  background-color #333 !important
  border-color #333 !important
  opacity 1 !important